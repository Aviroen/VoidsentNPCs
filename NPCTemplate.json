          "Actions": [
            "Spiderbuttons.BETAS_SetNewDialogue {{ModId}}_{{NPCNAME}} \"{{i18n:BETAS.{{NPCNAME}}.{{WeekDay}}_{{Random:{{Range:4,{{ENDRANGE}}}}}}}}\""
